      <RecipeDef>
      <defName>MakeBasicInformationChip</defName>
      <label>fabricate Basic Information Chip</label>
      <description>Fabricate a Basic Information Chip, which increases skill gain.</description>
      <jobString>Fabricating Basic Information Chip.</jobString>
      <workAmount>3500</workAmount>
      <workSpeedStat>SmoothingSpeed</workSpeedStat>
      <effectWorking>ConstructMetal</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workSkill>Crafting</workSkill>
      <skillRequirements>
      <Crafting>10</Crafting>
      </skillRequirements>
      <ingredients>
        <li>
          <filter>
            <thingDefs>
              <li>Plasteel</li>
            </thingDefs>
          </filter>
          <count>40</count>
        </li>   
        <li>
          <filter>
            <thingDefs>
              <li>ComponentIndustrial</li>
            </thingDefs>
          </filter>
          <count>4</count>
        </li>
        <li>
          <filter>
            <thingDefs>
              <li>MedicineIndustrial</li>
            </thingDefs>
          </filter>
          <count>6</count>
        </li>
		<li>
          <filter>
            <thingDefs>
              <li>BasicChipset</li>
            </thingDefs>
          </filter>
          <count>1</count>
        </li>
      </ingredients>
      <products>
        <BasicInformationChip>1</BasicInformationChip>
      </products>
      <recipeUsers>
        <li>ElectricAssembler</li>
      </recipeUsers>
	  <researchPrerequisite>ResearchB</researchPrerequisite>
    </RecipeDef>

