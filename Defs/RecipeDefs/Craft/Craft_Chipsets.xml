<Defs>
  
    <RecipeDef>
      <defName>MakeSolder</defName>
      <label>smelt solder</label>
      <description>Smelt Steel and Silver to create the alloy Solder.</description>
      <jobString>Making alloy.</jobString>
      <workAmount>250</workAmount>
      <workSpeedStat>SmoothingSpeed</workSpeedStat>
      <effectWorking>ConstructMetal</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workSkill>Crafting</workSkill>
      <skillRequirements>
      <Crafting>5</Crafting>
      </skillRequirements>
      <ingredients>
        <li>
          <filter>
            <thingDefs>
              <li>Steel</li>
            </thingDefs>
          </filter>
          <count>10</count>
        </li>       
        <li>
          <filter>
            <thingDefs>
              <li>Silver</li>
            </thingDefs>
          </filter>
          <count>1.5</count>
        </li>
      </ingredients>
      <products>
        <Solder>1</Solder>
      </products>
      <recipeUsers>
        <li>ElectronicsWorkbench</li>
      </recipeUsers>
	  <researchPrerequisite>Electronics</researchPrerequisite>
    </RecipeDef>
  
