<Defs>

      <RecipeDef>
      <defName>MakeAdvancedInformationChip</defName>
      <label>fabricate Advanced Information Chip</label>
      <description>Fabricate an Advanced Information Chip, which increases skill gain.</description>
      <jobString>Fabricating Advanced Information Chip.</jobString>
      <workAmount>5500</workAmount>
      <workSpeedStat>SmoothingSpeed</workSpeedStat>
      <effectWorking>ConstructMetal</effectWorking>
      <soundWorking>Recipe_Machining</soundWorking>
      <workSkill>Crafting</workSkill>
      <skillRequirements>
      <Crafting>14</Crafting>
      </skillRequirements>
      <ingredients>
        <li>
          <filter>
            <thingDefs>
              <li>Gold</li>
            </thingDefs>
          </filter>
          <count>3.5</count>
        </li>   
        <li>
          <filter>
            <thingDefs>
              <li>ComponentSpacer</li>
            </thingDefs>
          </filter>
          <count>2</count>
        </li>
		<li>
          <filter>
            <thingDefs>
              <li>AdvancedChipset</li>
            </thingDefs>
          </filter>
          <count>1</count>
        </li>
      </ingredients>
      <products>
        <AdvancedInformationChip>1</AdvancedInformationChip>
      </products>
      <recipeUsers>
        <li>ElectricAssembler</li>
      </recipeUsers>
	  <researchPrerequisite>InformB</researchPrerequisite>
    </RecipeDef>

